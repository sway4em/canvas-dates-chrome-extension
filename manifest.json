{
    "manifest_version": 3,
    "name": "Canvas Countdown",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "*://*.instructure.com/*",
        "*://*.edu/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "icon.png"
    }
}